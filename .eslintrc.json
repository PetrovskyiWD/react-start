{
	"extends": ["airbnb", "prettier", "prettier/react", "eslint:recommended"],
	"parser": "babel-eslint",
	"plugins": ["react", "prettier"],
	"rules": {
		"react/jsx-filename-extension": [
			1,
			{
				"extensions": [".js", "jsx"]
			}
		],
		"prettier/prettier": "error",
		"max-len": ["error", 120],
		"import/no-unresolved": "off",
		"import/prefer-default-export": "off",
		"no-console": "off"
	},
	"env": {
		"browser": true
	}
}
